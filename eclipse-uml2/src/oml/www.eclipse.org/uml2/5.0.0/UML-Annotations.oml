@dc:^description "The UML annotations vocabulary"
vocabulary <http://www.eclipse.org/uml2/5.0.0/UML-Annotations> with # as uml-annotations {

	extends <http://www.w3.org/2002/07/owl>

	@dc:^description "An annotation on an entity specifying it is a stereotype of some UML metaclass" 
	annotation property isStereotypeOf

	@dc:^description "An annotation on an entity specifying it is a profile-based UML class" 
	annotation property isClass

}